  keywords = {Abbasid coinage,classical coinage system,currency system,Islamic coins,Islamic legal theory,Ismaili Shiite Fatimids,monetary economy,Numismatics,Umayyad regime,Zubayrid governor},
  author = {Heidemann, Stefan},
  editor = {Robinson, Chase F.},
  doi = {10.1017/CHOL9780521838238.018}
}

@incollection{BonnerWaning2010,
