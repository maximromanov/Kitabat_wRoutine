  pages = {623-647},
  keywords = {contemporary scholars,early Islamic history,Enlightenment thinkers,nationalist conceptualisations,practical impediments,Western approaches},
  author = {Donner, Fred},
  editor = {Robinson, Chase F.},
  doi = {10.1017/CHOL9780521838238.017}
}

@incollection{HeidemannNumismatics2010,
