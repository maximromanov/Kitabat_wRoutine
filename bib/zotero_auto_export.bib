  series = {The {{New Cambridge History}} of {{Islam}}},
  publisher = {{Cambridge University Press}},
  date = {2010},
  pages = {144-158},
  keywords = {West Africa,Islamisation,Sudan,Arabic language,Almoravid reform movement},
  author = {Rebstock, Ulrich},
  editor = {Fierro, Maribel},
  doi = {10.1017/CHOL9780521839570.007}
}

@incollection{PeskesWestern2010,
  location = {{Cambridge}},
  title = {Western {{Arabia}} and {{Yemen}} (Fifth/Eleventh Century to the {{Ottoman}} Conquest)},
  volume = {2},
  isbn = {978-0-521-83957-0},
  url = {https://www.cambridge.org/core/books/new-cambridge-history-of-islam/western-arabia-and-yemen-fiftheleventh-century-to-the-ottoman-conquest/36AB30CE1310C04DF66C8C66A08BB5FE},
  abstract = {From the second half of the fourth/tenth century the ˓Abbāsid caliphate's hegemonic monopoly in western Arabia eroded. The economic and geostrategic importance of the Red Sea intensified in the wake of the ˓Abbāsid state's decline, and the basic significance of the Ḥijāz as goal of the pilgrimage and original abode of Islam widened. The Mamlūks incorporated the sharīfate into the Mamlūk state structure by appointing the ruler of Mecca to the newly created office of vice-sultan of the Ḥijāz in 811/1408. At the beginning of the fifth/eleventh century, Yemen was split up between several political forces none of which was strong enough to overcome regional competitors. Najāḥids, Zaydīs and Ṣulayḥids were to determine politics in Yemen in the fifth/eleventh century. The Zaydīs extended their territories towards the south, while members of the Ṭāhirids clan held power in Aden and its hinterland and remaining Egyptian Mamlūks who displayed loyalty towards the Ottoman sultan established themselves in Zabīd.},
  booktitle = {The {{New Cambridge History}} of {{Islam}}: {{Volume}} 2: {{The Western Islamic World}}, {{Eleventh}} to {{Eighteenth Centuries}}},
  series = {The {{New Cambridge History}} of {{Islam}}},
  publisher = {{Cambridge University Press}},
  date = {2010},
  pages = {285-298},
  keywords = {western Arabia,Yemen,Ottoman dynasty,Ṣulayḥids,Najāḥids,Zabīd,Zaydīs},
  author = {Peskes, Esther},
  editor = {Fierro, Maribel},
  doi = {10.1017/CHOL9780521839570.011}
}

@incollection{TouatiOttoman2010,
