  pages = {581-622},
  keywords = {North Africa,Iberian Peninsula,Islamisation,Berber revolt,eastern dynasties,eastern ideologies,Kharijism,territorial expansion,Umayyad administration,western caliphates},
  author = {Moreno, Eduardo},
