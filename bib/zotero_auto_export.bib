  url = {https://www.cambridge.org/core/books/new-cambridge-history-of-islam/conversion-to-islam/915B1DDCB263DE8027AC6D703639A959},
  abstract = {Over the centuries many millions of people have changed their religious identity from non-Muslim to Muslim. However, scholars understand the word 'conversion' in a variety of ways and use differing approaches in their studies of these changes. The study of conversion to Islam is burdened by tales and assumptions that have little basis in fact. No general history of the whens, hows and whys of conversion is possible. In the place of such a history, this chapter presents an assessment of current analytical approaches and of the types of conclusions that can be reached through different methods: quantitative, economic, literary analysis, and institutional. Even if the sources were available to determine the precise percentages of Muslims in the Central Asian, Chinese, Indian or south-eastern populations at any point in time, the characteristics of the faith professed by those Muslims would be seen to change over time.},
  booktitle = {The {{New Cambridge History}} of {{Islam}}: {{Volume}} 3: {{The Eastern Islamic World}}, {{Eleventh}} to {{Eighteenth Centuries}}},
  series = {The {{New Cambridge History}} of {{Islam}}},
  publisher = {{Cambridge University Press}},
  date = {2010},
  pages = {527-538},
  keywords = {Chinese Muslims,Indian Muslims,Central Asian Muslims,south-eastern Muslims,Islamic conversion},
  author = {Bulliet, Richard},
  editor = {Reid, Anthony and Morgan, David O.},
  doi = {10.1017/CHOL9780521850315.017}
}

@incollection{SubtelnyTamerlane2010,
  location = {{Cambridge}},
  title = {Tamerlane and His Descendants: From Paladins to Patrons},
  volume = {3},
  isbn = {978-0-521-85031-5},
  url = {https://www.cambridge.org/core/books/new-cambridge-history-of-islam/tamerlane-and-his-descendants-from-paladins-to-patrons/AD477830D8C1809A9D67784F1EB5F93E},
  abstract = {The restoration of political order and the re-establishment of balance in the eastern Islamic world was the result of the ruthless empire-building strategies of the nomadic warlord Tamerlane or Temur. Temur's authority was based on the Turko-Mongolian concept of leadership based on charisma, which represents one of the forms of legitimate domination in history. He also linked himself genealogically to Chinggis Khan, the founder of the Mongol empire. Although Temur did not hesitate to invoke Islamic legality whenever it was to his advantage to do so, Temur generally favoured the yasa over the sharia, the Islamic law. The Timurid state was predominantly military in character. It was based on a tribal military elite and supported by nomadic cavalry forces. The upsurge in cultural activity in Iran and Central Asia under Timurid patronage coincided roughly with the Renaissance in Europe; historians of Timurid art and culture dubbed it as the Timurid renaissance.},
  booktitle = {The {{New Cambridge History}} of {{Islam}}: {{Volume}} 3: {{The Eastern Islamic World}}, {{Eleventh}} to {{Eighteenth Centuries}}},
  series = {The {{New Cambridge History}} of {{Islam}}},
  publisher = {{Cambridge University Press}},
  date = {2010},
  pages = {169-200},
  keywords = {Chinggisid tradition,cultural patronage,charismatic authority,yasa,Timurid state,paladin,Islamic law,Tamerlane,sharia},
  author = {Subtelny, Maria},
  editor = {Reid, Anthony and Morgan, David O.},
  doi = {10.1017/CHOL9780521850315.007}
}

@incollection{DaleIndia2010,
  location = {{Cambridge}},
  title = {India under {{Mughal}} Rule},
  volume = {3},
  isbn = {978-0-521-85031-5},
  url = {https://www.cambridge.org/core/books/new-cambridge-history-of-islam/india-under-mughal-rule/8BD7339C202A29263F2ECD67F3D9B7B0},
  abstract = {The Mughals' history of internecine dynastic warfare implicitly raises one facet of the dynasty's Central Asian heritage, their military tactics. Babur marched out of Kabul in 932/December 1525 intent on conquering Hindustan; in 1526, he founded the Mughul empire in India. Humayun's young son Akbar succeeded to the Mughal throne in 963/1556 in his twelfth year. The reign of Akbar's successor Salim or Jahangir, the 'World-seizer', vividly illustrates the importance of idiosyncratic personality and also witnessed the beginnings of Sikhism as a social and political force. Shah Jahan turned his back on Akbar's and Jahangir's sympathetic attitude towards non-Muslims, and he also exhibited little of the relentless intellectual curiosity of Akbar or the deeply emotional artistic sensitivity of Jahangir. One of the earliest signs of Shah Jahan's lofty ambition was the construction of the Peacock Throne, a gem-encrusted monument that was commissioned in 1037/1628 and completed seven years later.},
  booktitle = {The {{New Cambridge History}} of {{Islam}}: {{Volume}} 3: {{The Eastern Islamic World}}, {{Eleventh}} to {{Eighteenth Centuries}}},
  series = {The {{New Cambridge History}} of {{Islam}}},
  publisher = {{Cambridge University Press}},
  date = {2010},
  pages = {266-314},
  keywords = {Babur,Central Asian heritage,Akbar,Sikhism,Shah Jahan,Mughal architecture,Mughal empire,Jahangir,Hindustan},
  author = {Dale, Stephen},
  editor = {Reid, Anthony and Morgan, David O.},
  doi = {10.1017/CHOL9780521850315.010}
}

