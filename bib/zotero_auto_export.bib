  keywords = {Abd al-Malik,Farghana,France,imperial expansion,Islamic Syria,Marwanid caliphate,Marwanid dynasty,Marwanid elites,Third Fitna,Yamani settlement},
  author = {Cobb, Paul},
  editor = {Robinson, Chase F.},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/CobbP/Cobb 2010 - The empire in Syria, 705–763 (V1).pdf},
