  keywords = {Arab settlers,Baghdad,Ghaznavids,Islamic east,political economy,regionalism,Saffarids,Samanids,Sasanian social structure,Tahirids},
  author = {Daniel, Elton},
  editor = {Robinson, Chase F.},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/DanielE/Daniel 2010 - The Islamic east (V1).pdf},
