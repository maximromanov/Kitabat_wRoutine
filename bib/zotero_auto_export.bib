  keywords = {Egypt,Ottoman empire,Anatolia,Islamic taxation,Mamlūk dynasty,military slavery,Saljuq empire},
  author = {Fuess, Albrecht},
  editor = {Fierro, Maribel},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/FuessA/Fuess 2010 - Taxation and armies (V2).pdf},
  doi = {10.1017/CHOL9780521839570.023}
}

@incollection{Viguera-MolinsAlAndalus2010,
