  keywords = {Iran's economy,Safavid era,European economic interests,Sunni population,Shii state},
  author = {Garthwaite, G. R.},
  editor = {Reid, Anthony and Morgan, David O.},
  doi = {10.1017/CHOL9780521850315.016}
}

@incollection{WinkEarly2010,
