  keywords = {Ottoman empire,Syria,Indian Ocean,Mamlūk,western Arabia,Yemen,Ḥijāz},
  author = {Haykel, Bernard},
  editor = {Fierro, Maribel},
  doi = {10.1017/CHOL9780521839570.017}
}

@incollection{RebstockWest2010,
