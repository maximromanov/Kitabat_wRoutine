  pages = {305-359},
  keywords = {civil war,Islamic empire,Abbasid caliphate,Abbasid power,Abbasid restoration,Buyids,Jaqfar al-Mutawakkil's assassination,ninth-century Ismailism,opposition movements,Samarra},
  author = {Bonner, Michael},
  editor = {Robinson, Chase F.},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/BonnerM/Bonner 2010 - The waning of empire, 861–945 (V1).pdf},
