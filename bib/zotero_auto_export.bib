  keywords = {Islamic empire,Russian Orthodox Christians,sectarian movements,Spanish Catholics,Balkan Peninsula,Asia Minor},
  author = {García-Arenal, Mercedes},
  editor = {Fierro, Maribel},
  doi = {10.1017/CHOL9780521839570.022}
}

@incollection{LevFatimid2010,
