@article{Rock-SingerScholarly2016,
  langid = {english},
  title = {Scholarly {{Authority}} and {{Lay Mobilization}}: {{Yusuf}} Al-{{Qaradawi}}'s Vision of Da‘wa, 1976-1984},
  volume = {106},
  issn = {1478-1913},
  url = {http://onlinelibrary.wiley.com/doi/10.1111/muwo.12135/abstract},
  doi = {10.1111/muwo.12135},
  shorttitle = {Scholarly {{Authority}} and {{Lay Mobilization}}},
  abstract = {The late 1970s and early 1980s were a propitious yet challenging time for the Muslim Brotherhood in Egypt, as its elites sought to define the movement's priorities in the face of new opportunities to spread their call (da‘wa). The debate over preaching, while one of strategic assessment, also involved a negotiation of intellectual hierarchy: Should laymen lead Egypt's oldest Islamist organization, or should scholars? In contrast to previous studies that focus on how laymen led the Brotherhood's return to grassroots preaching, this article reintegrates scholars into the story of da‘wa by focusing on the organization's most prominent ‘ālim, Shaykh Yusuf al-Qaradawi, and his vision of institution-based preacher education and extra-institutional activism. Drawing on three books written by Qaradawi on this topic between the mid-1970s and early 1980s, this article casts lights not only on this Islamist scholar's claim to religious authority as he sought to mold the Brotherhood, but also on the ways in which projects of mass mobilization – whether grassroots preaching or the reform of state-sponsored educational curricula – have transformed scholarly claims to authority more broadly.},
  number = {3},
  journaltitle = {The Muslim World},
  shortjournal = {Muslim World},
  urldate = {2016-07-06},
  date = {2016-07-01},
  pages = {588-604},
  author = {Rock-Singer, Aaron},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/Rock-SingerA/Rock-Singer 2016 - Scholarly Authority and Lay Mobilization (V106).pdf;/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroApp/storage/JNVUJJ7H/abstract.html}
}

