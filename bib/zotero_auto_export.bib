  series = {The {{New Cambridge History}} of {{Islam}}},
  publisher = {{Cambridge University Press}},
  date = {2010},
  pages = {98-152},
  keywords = {Ardashir,Byzantine military,Iranian kings,late Sasanian Near East,Parthian empire,religious policy,royal court,Yazdgerd III},
  author = {Wiesehöfer, Josef},
  editor = {Robinson, Chase F.},
